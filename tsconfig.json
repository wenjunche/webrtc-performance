{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext", "webworker"],
    "target": "es2019",
    "module": "esnext",
    "outDir": "dist",
    "rootDir": ".",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "jsx": "react"
  },
  "include": ["*.ts", "example.tsx"],
  "exclude": ["node_modules"]
}
